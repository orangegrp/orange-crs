{
  "name": "orange-crs",
  "version": "1.0.0",
  "description": "orangeðŸŸ  Code Runner Server",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "build-all": "cd local_modules/orange-common-lib && tsc && cd ../.. && tsc",
    "dev": "tsc && node dist/index.js",
    "prod": "tsc && NODE_ENV=production node dist/index.js",
    "prod-win": "tsc && set NODE_ENV=production && node dist/index.js",
    "gen-secret": "node -e \"console.log(require('crypto').randomBytes(32).toString('hex'))\"",
    "gen-token": "node -e \"console.log(require('jsonwebtoken').sign({id: process.argv[2]}, process.argv[1], {expiresIn:'1y'}))\"",
    "verify-token": "node -e \"console.log(require('jsonwebtoken').verify(process.argv[1], process.argv[2]))\""
  },
  "author": "",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "fastify": "^4.25.2",
    "jsonwebtoken": "^9.0.2",
    "orange-common-lib": "file:local_modules/orange-common-lib"
  }
}